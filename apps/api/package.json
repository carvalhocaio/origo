{
  "name": "api",
  "version": "1.0.0",
  "description": "Origo API with Fastify and Bun",
  "type": "module",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun --external '*'",
    "start": "bun src/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "format": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.6.0",
    "@scalar/fastify-api-reference": "^1.39.3",
    "fastify": "^5.6.2",
    "fastify-type-provider-zod": "^6.1.0",
    "zod": "^4.1.12"
  }
}
